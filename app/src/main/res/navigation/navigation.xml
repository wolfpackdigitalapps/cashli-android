<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/splashScreenFragment">

    <fragment
        android:id="@+id/splashScreenFragment"
        android:name="com.wolfpackdigital.cashli.presentation.main.splash.SplashScreenFragment"
        android:label="SplashScreenFragment"
        tools:layout="@layout/fr_splash_screen" >
        <action
            android:id="@+id/action_splashScreenFragment_to_listFragment"
            app:destination="@id/listFragment" />
    </fragment>
    <fragment
        android:id="@+id/listFragment"
        android:name="com.wolfpackdigital.cashli.presentation.main.list.ListFragment"
        android:label="ListFragment"
        tools:layout="@layout/fr_list" />
</navigation>