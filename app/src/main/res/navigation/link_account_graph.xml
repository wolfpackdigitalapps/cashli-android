<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/link_account_graph"
    app:startDestination="@id/linkBankAccountInformativeFragment">

    <include app:graph="@navigation/claim_cash_graph" />

    <action
        android:id="@+id/action_global_claimCashGraph"
        app:destination="@id/claim_cash_graph" />

    <fragment
        android:id="@+id/linkBankAccountInformativeFragment"
        android:name="com.wolfpackdigital.cashli.presentation.linkBank.informative.LinkBankAccountInformativeFragment"
        android:label="LinkBankAccountInformativeFragment"
        tools:layout="@layout/fr_link_bank_account_informative">
        <action
            android:id="@+id/action_linkBankAccountInformativeFragment_to_ineligibleInformativeFragment"
            app:destination="@id/ineligibleInformativeFragment" />
    </fragment>
    <fragment
        android:id="@+id/ineligibleInformativeFragment"
        android:name="com.wolfpackdigital.cashli.presentation.linkBank.ineligible.IneligibleInformativeFragment"
        android:label="IneligibleInformativeFragment"
        tools:layout="@layout/fr_ineligible_informative">
        <deepLink app:uri="android-app://com.wolfpackdigital.cashli/ineligibleInformative" />
    </fragment>

</navigation>