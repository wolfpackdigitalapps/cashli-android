<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners
                android:topLeftRadius="@dimen/dimen_20dp"
                android:topRightRadius="@dimen/dimen_20dp" />
            <solid android:color="@color/colorWhiteF5" />
            <stroke
                android:width="@dimen/dimen_1dp"
                android:color="@color/colorGrayDC" />
        </shape>
    </item>
    <item
        android:end="@dimen/dimen_1dp"
        android:start="@dimen/dimen_1dp"
        android:top="@dimen/dimen_1dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/colorWhiteF5" />
            <corners
                android:topLeftRadius="@dimen/dimen_20dp"
                android:topRightRadius="@dimen/dimen_20dp" />
        </shape>
    </item>
</layer-list>