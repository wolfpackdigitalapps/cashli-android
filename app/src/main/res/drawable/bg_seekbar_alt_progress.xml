<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/progress">
        <scale android:scaleWidth="100%">
            <shape android:shape="rectangle">
                <corners android:radius="@dimen/dimen_10dp" />
                <gradient
                    android:centerColor="@color/claimCashSeekbarAltGradientCenter"
                    android:endColor="@color/claimCashSeekbarAltGradientEnd"
                    android:startColor="@color/claimCashSeekbarAltGradientStart" />
            </shape>
        </scale>
    </item>

    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/dimen_10dp" />
            <stroke
                android:width="1dp"
                android:color="@color/colorGrayB6" />
        </shape>
    </item>

    <item android:id="@android:id/secondaryProgress">
        <scale android:scaleWidth="100%">
            <shape android:shape="rectangle">
                <corners android:radius="@dimen/dimen_10dp" />
                <solid android:color="@android:color/transparent" />
            </shape>
        </scale>
    </item>
</layer-list>
